{
  "name": "backend",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/index.js",
    "start:hmr": "node dist/server",
    "docker-build": "mkdir dist && mkdir dist/graphqls && cp src/graphqls/*.graphql dist/graphqls/ && mkdir dist/config && cp src/config/docker.js dist/config/ && tsc",
    "docker-run": "pm2-docker start ./index.js -i 1 --max-memory-restart 240M --raw",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pre:webpack": "rimraf dist && mkdir dist && cp src/graphqls/*.graphql dist/ && cp src/config/*.json5 dist/",
    "webpack": "npm run pre:webpack && webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/graphql": "^5.4.0",
    "@s4p/music-api": "^1.14.0",
    "@s4p/nest-nmdb": "^0.0.1",
    "@s4p/nmdb": "^0.3.0",
    "apollo-server-express": "^2.1.0",
    "audio-decode": "^1.4.0",
    "cors": "^2.8.5",
    "fs-extra": "^7.0.1",
    "got": "^9.6.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.3",
    "graphql-type-json": "^0.2.1",
    "joi": "^14.3.0",
    "lodash": "^4.17.11",
    "mediaserver": "^0.1.1",
    "pino": "^5.10.1",
    "pino-pretty": "^2.5.0",
    "promise-retry": "^1.1.1",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "rxjs": "^6.2.2",
    "through2": "^3.0.0",
    "typescript": "^3.1.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/camo": "^0.12.0",
    "@types/express": "^4.16.0",
    "@types/got": "^9.2.0",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.9",
    "@types/joi": "^14.0.1",
    "@types/lodash": "^4.14.121",
    "@types/node": "^10.12.1",
    "@types/pino": "^5.8.3",
    "@types/supertest": "^2.0.5",
    "@types/through2": "^2.0.34",
    "jest": "^29.3.1",
    "nodemon": "^1.18.5",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^1.7.2"
  }
}
